				{
					"name": "Criar transação",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.sendRequest({\r",
									"url: pm.globals.get(\"transacaoDomain\") + '/api/accounts/login',\r",
									"method: 'POST',\r",
									"header: {\r",
									"'content-type': 'application/json'\r",
									"},\r",
									"body: {\r",
									"mode: 'raw',\r",
									"raw: JSON.stringify({ \r",
									"email: pm.globals.get(\"emailAccount\"), \r",
									"senha: pm.globals.get(\"senhaAccount\") })\r",
									"}\r",
									"}, function (err, res) {\r",
									"    const cabecalho = {...res.headers}\r",
									"    console.log('oi')\r",
									"    console.log(res)\r",
									"    console.log(cabecalho.members[1].value)\r",
									"    pm.globals.set(\"token\" , 'Bearer' + cabecalho.members[1].value);\r",
									"});\r",
									""
								],
								"type": "text/javascript"
							}
